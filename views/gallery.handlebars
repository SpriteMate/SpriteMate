
<h1>Gallery Page</h1>

{{#each Sprites}}
	{{#unless this.Private}}
		<div class="row">
		  <div class="col-sm-6 col-md-4">
		    <div class="thumbnail">
		      <img src={{this.Image}} alt="...">
		      <div class="caption">
		        <h3>Sprite Name</h3>
		        <p> Description{{this.Description}} </p>
		        <p><a href="#" class="btn btn-primary" role="button">Save</a> <a href="#" class="btn btn-default" role="button">Preview</a></p>
		      </div>
		    </div>
		  </div>
		</div>
	{{/unless}}	
{{/each}}	



{{!-- Result Pages --}}
